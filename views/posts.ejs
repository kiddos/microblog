<div class="container row">
<% posts.forEach(function(post, index) {  %>
  <div class="col-sm-6 col-md-4 col-lg-4 col-xs-12">
    <div class="container-fluid row">
      <h2><a href="/u/<%= post.user %>"><%= post.user %></a> èªª</h2>
      <p><small><%= post.time %></small></p>
      <p><%= post.post %></p>
      <% if (post.image) { %>
      <img src="/img/posts/<%= post.image %>"
        class="col-sm-12 col-xs-12 col-md-12 col-lg-12">
      <% } %>
    </div>
  </div>
<%}) %>
</div>
